@article{Makogon2010,
abstract = {Topological insulators are a broad class of unconventional materials that are insulating in the interior but conduct along the edges. This edge transport is topologically protected and dissipationless. Until recently, all existing topological insulators, known as quantum Hall states, violated time-reversal symmetry. However, the discovery of the quantum spin Hall effect demonstrated the existence of novel topological states not rooted in time-reversal violations. Here, we lay out an experiment to realize time-reversal topological insulators in ultra-cold atomic gases subjected to synthetic gauge fields in the near-field of an atom-chip. In particular, we introduce a feasible scheme to engineer sharp boundaries where the "edge states" are localized. Besides, this multi-band system has a large parameter space exhibiting a variety of quantum phase transitions between topological and normal insulating phases. Due to their unprecedented controllability, cold-atom systems are ideally suited to realize topological states of matter and drive the development of topological quantum computing.},
author = {Makogon, D and Spielman, I B and Smith, C Morais},
title = {{Spin-charge-density wave in a squircle-like Fermi surface for ultracold atoms}},
year = {2010}
}
@article{Joshi2014,
abstract = {Using a round-robin data set assembled from over 60 teams of more than 500 scientists and engineers across a variety of science and engineering disciplines, as well as longitudinal research productivity data, this study examines differences in how men and women in science and engineering teams evaluate their colleagues' expertise and how that affects team performance. Because these teams are assembled to enhance innovations, they are most productive if they fully utilize the expertise of all team members. Applying a social relations modeling approach, two studies conducted in multidisciplinary research centers in a large public U. S. university test whether a team's gender composition predicts how well women's expertise is used within the team, based on peer evaluations of male and female team members with varying education levels. A third study returns to the same two research centers to examine whether the larger context in which the team operates affects the use of expertise and the team's productivity. An important finding is that the gender and educational attributes of the person being evaluated are less critical to the recognition of expertise than the attributes of the person conducting the evaluation and the relationship between these two team members. In addition, context matters: gender-integrated teams with a higher proportion of highly educated women are more productive in disciplines with a greater female faculty representation.},
author = {Joshi, a.},
doi = {10.1177/0001839214528331},
file = {:C$\backslash$:/Users/lma2/Desktop/Sexual Harrassment research/Joshi ASQ{\_}By Whom and When is Women's Expertise Recognized.pdf:pdf},
isbn = {0001-8392},
issn = {0001-8392},
journal = {Administrative Science Quarterly},
keywords = {DIFFERENCE,DIVERSITY,FEMALE LEADERS,LEGITIMACY,PERFORMANCE,PSYCHOLOGY,SELF-CATEGORIZATION,SOCIAL IDENTITY,STEREOTYPE THREAT,WORK GROUPS,expertise recognition,gender-integrated teams,science and engineering teams,team performance,women's expertise},
number = {2},
pages = {202--239},
title = {{By Whom and When Is Women's Expertise Recognized? The Interactive Effects of Gender and Education in Science and Engineering Teams}},
url = {http://apps.webofknowledge.com/full{\_}record.do?product=UA{\&}search{\_}mode=GeneralSearch{\&}qid=2{\&}SID=V1jhcSyC6wjtlotF9WK{\&}page=2{\&}doc=12{\&}cacheurlFromRightClick=no},
