number = {4},
pages = {45006},
title = {{The trapped two-dimensional Bose gas: from Bose‚{\{}{\"{A}}{\}}{\{}{\`{\i}}{\}}Einstein condensation to Berezinskii‚{\{}{\"{A}}{\}}{\{}{\`{\i}}{\}}Kosterlitz‚{\{}{\"{A}}{\}}{\{}{\`{\i}}{\}}Thouless physics}},
volume = {10},
year = {2008}
}
