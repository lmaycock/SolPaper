@article{Moore2007,
abstract = {{\{}T{\}}he construction of a laser cooling/trapping apparatus with a versatile$\backslash$nmm-scale magnetic trap for ultracold atoms is described herein. {\{}T{\}}he$\backslash$ndesign, operation, and performance of this unique trap are presented.$\backslash$n{\{}T{\}}he manipulation of this magnetic trapping system facilitated the$\backslash$n{\{}B{\}}ose-condensation of {\{}{\}}{\^{}}{\{}87{\}}{\{}R{\}}b atoms in a variety of magnetic$\backslash$ntraps, most notably a millimeter radius circular magnetic trap for$\backslash$nultracold atoms. {\{}T{\}}he dynamics of the quantum degenerate atom beam$\backslash$nin this geometry are explored, as well as future applications with$\backslash$nrefinements of this system. {\{}A{\}} new probe of the phase space distribution$\backslash$nof a generalized atomic beam is presented, and this probe was employed$\backslash$nin the circular magnetic waveguide to characterize the quantum state$\backslash$nof the system. {\{}F{\}}inally, this mm-scale magnetic trap was integrated$\backslash$nwith a mm-scale high-finesse optical cavity which accesses the strong$\backslash$ncoupling regime of cavity quantum electrodynamics ({\{}QED{\}}). {\{}L{\}}arge$\backslash$nensembles of ultracold atoms were delivered to this cavity, and the$\backslash$nfirst experimental results of this new dispersive regime of many-atom$\backslash$ncavity {\{}QED{\}} are described.},
