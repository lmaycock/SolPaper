journal = {Cultural Studies of Science Education},
keywords = {Competence,Discourses,Gender,Physics education,Positioning,Recognition},
number = {2},
pages = {503--521},
title = {{"Physics and the girly girl-there is a contradiction somewhere": Doctoral students' positioning around discourses of gender and competence in physics}},
volume = {9},
